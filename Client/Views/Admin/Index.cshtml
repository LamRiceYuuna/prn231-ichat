}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <script src="~/js/admin.js"></script>
    <link rel="icon" type="image/png" href="~/assetsadmin/img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Light Bootstrap Dashboard by Creative Tim</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    <link href="~/assetsadmin/css/bootstrap.min.css" rel="stylesheet" />

    <link href="~/assetsadmin/css/animate.min.css" rel="stylesheet" />

    <link href="~/assetsadmin/css/light-bootstrap-dashboard.css?v=1.4.0" rel="stylesheet" />


    <link href="~/assetsadmin/css/demo.css" rel="stylesheet" />


    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="~/assetsadmin/css/pe-icon-7-stroke.css" rel="stylesheet" />
</head>
<body>

    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="~/assetsadmin/img/sidebar-5.jpg">
         
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="#" class="simple-text">
                        Creative Tim
                    </a>
                </div>

                <ul class="nav">
                    <li class="active">
                        <a href="#">
                            <i class="pe-7s-graph"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>                  
                    <li class="active-pro" id="log-out">
                        <a href="#">
                            <i class="pe-7s-rocket"></i>
                            <p>LOG OUT</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="main-panel">
            
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="card">

                                <div class="header">
                                    <h4 class="title">Statistical access</h4>                                   
                                </div>
                                <div class="content">
                                    <div id="chartPreferences" class="ct-chart ct-perfect-fourth"></div>

                                    <div class="footer">
                                        <div class="legend">
                                            <i class="fa fa-circle text-info"></i> Open
                                            <i class="fa fa-circle text-danger"></i> Bounce
                                            <i class="fa fa-circle text-warning"></i> Unsubscribe
                                        </div>
                                        <hr>
                                       
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-8">
                            <div class="card">
                                <div class="header">
                                    <h4 class="title">Number of messages</h4>
                                    <p class="category">24 Hours performance</p>
                                </div>
                                <div class="content">
                                    <div id="chartHours" class="ct-chart"></div>
                                    <div class="footer">
                                        <div class="legend">
                                            
                                        </div>
                                        <hr>
                                        <div class="stats">                                          
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>



                    <div class="row">
                        <div class="col-md-12">
                            <div class="card ">
                                <div class="header">
                                    <h4 class="title">Monthly traffic.</h4>
                                    <p class="category"></p>
                                </div>
                                <div class="content">
                                    <div id="chartActivity" class="ct-chart"></div>

                                    <div class="footer">
                                        <div class="legend">
                                            
                                        </div>
                                        <hr>
                                        <div class="stats">
                                            <i class="fa fa-check"></i> Data information certified
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="card ">

                                <div class="header">

                                    <h4 class="title">User List</h4>
                                    <p class="category">Manage users</p>
                                </div>

                                <div class="content">
                                    <div class="table-full-width">

                                        <table class="table" id="user-band">

                                            <thead>
                                                <tr>

                                                    <th>UUID</th>
                                                    <th>NickName</th>
                                                    <th>Username</th>
                                                    <th>Email</th>
                                                    <th>IsEmailConfirmed</th>
                                                    <th>HasPassword</th>
                                                    <th>LastLogin</th>
                                                    <th>Role</th>
                                                    <th class="text-right">Action</th>

                                                </tr>
                                            </thead>
                                            <tbody> 
                                            </tbody>
                                        </table>

                                    </div>

                                    <div class="footer">

                                        <hr>
                                        <div class="stats">
                                        </div>

                                    </div>

                                </div>

                            </div>
                        </div>
@*                         public string UUID { get; set; }
                        public string Username {  get; set; }
                        public string Email {  get; set; }
                        public DateTime LastLogin { get; set; }
                        public string NickName {  get; set; }
                        public string AvatarUrl {  get; set; }
                        public TimeSpan AccessTime {  get; set; } *@

                        <div class="col-md-12">
                            <div class="card ">

                                <div class="header">

                                    <h4 class="title">User Statistic In Week</h4>
                                    <p class="category">User traffic</p>
                                </div>

                                <div class="content">
                                    <div class="table-full-width" id="user-statistic-container">

                                        <table class="table" id="user-statistic">

                                            <thead>
                                                <tr>

                                                    <th>UUID</th>
                                                    <th>Username</th>
                                                    <th>NickName</th>
                                                    <th>Email</th>     
                                                    <th>LastLogin</th>
                                                    <th>AccessTime</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            </tbody>
                                        </table>

                                    </div>

                                    <div class="footer">

                                        <hr>
                                        <div class="stats">
                                        </div>

                                    </div>

                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </div>
           
        </div>
    </div>


</body>

<script src="~/assetsadmin/js/jquery.3.2.1.min.js" type="text/javascript"></script>
<script src="~/assetsadmin/js/bootstrap.min.js" type="text/javascript"></script>

<!--  Charts Plugin -->
<script src="~/assetsadmin/js/chartist.min.js"></script>

<!--  Notifications Plugin    -->
<script src="~/assetsadmin/js/bootstrap-notify.js"></script>

<!--  Google Maps Plugin    -->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>

<!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
<script src="~/assetsadmin/js/light-bootstrap-dashboard.js?v=1.4.0"></script>

<!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
<script src="~/assetsadmin/js/demo.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        demo.initChartist();

        $.notify({
            icon: 'pe-7s-gift',
            message: "Welcome to <b>Light Bootstrap Dashboard</b> - a beautiful freebie for every web developer."

        }, {
            type: 'info',
            timer: 4000
        });

    });
</script>

</html>
